    "license": "MIT",

    "repository": "vbauer/manet",
    "homepage": "https://github.com/vbauer/manet",
    "bugs": "https://github.com/vbauer/manet/issues",

    "main": "./src/runner.js",

    "bin": {
        "manet": "./bin/manet"
    },

    "author": {
